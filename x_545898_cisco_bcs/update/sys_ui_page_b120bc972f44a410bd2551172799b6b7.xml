	$j(".error-div").html('');
});
$j("textarea").click(function() {
	$j(".error-div").html('');
});


function validate_form() {
	
	var flag = true;
	for(var i = 1; i < 7; i++) {
		var rating = $j("input[name='radio_group"+i+"']:checked").val();
		var isEmpty = $j('#comment'+i).val().trim().length <= 0;
		if((!rating || rating < 3) && isEmpty) {
			$j("#error_display_div"+i).append('<div class="alert alert-danger" style="padding: 5px 16px; position: relative; margin: 5px 0px;">Please fill in all the fields</div>');
			flag = false;
		}
	}
}]]></client_script>
