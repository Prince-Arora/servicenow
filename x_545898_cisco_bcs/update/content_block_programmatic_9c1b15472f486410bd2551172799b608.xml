<j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="null">
<div class="form-group" style="height: 425px; overflow: hidden;"  onclick="location.href='ui_page.do?sys_id=1e3e64652f902010bd2551172799b6a9'">
