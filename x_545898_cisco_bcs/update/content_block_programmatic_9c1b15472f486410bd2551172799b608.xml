	<div style="height: 1%;"/>
</div>
<script>
	var ga = new GlideAjax('Cisco_ClientServerBridge');
	ga.addParam('sysparm_name', 'getDevices');
	ga.getXML(getAssetResponse);
	function getAssetResponse(response) {
		var answer = response.responseXML.documentElement.getAttribute("answer");
		var result = JSON.parse(answer);
		$j("#device_count").html('<h1>'+result[1]+'</h1>'+'<b>Unreachable device out of &#160; <label style="font-size: large;"><strong>'+ result[2]+'</strong></label></b>');
		var name = 'Config Status';
		var json = {};
		json[name] = [result[0]];
		var data_array = [];
		data_array.push({
		name: 'Config Status' + ' [' + result[0] + ']',
		y: parseInt(result[0])
});
		renderUIScript("device_overview"," ", data_array, "bar", "Vulnerabilities"," ");
	
function renderUIScript(groupBy,title,data,graphType,xAxis,yAxis) {
		ScriptLoader.getScripts('x_545898_cisco_bcs.Cisco_RenderGraph.jsdbx', function() {
		x_545898_cisco_bcs.Cisco_RenderGraph.renderBarChartOnWidget(groupBy,title,data,graphType,xAxis,yAxis);
});
}
chart.reflow();
}
</script>
